settings.outformat = "pdf";
size(x = 7.5cm, 5cm);

pair A = (0, 0);
pair B = (3, 0);
pair C = (3, 2);
pair D = (6, 2);
pair E = (3,-2);

path AB = A--B;
path BC = B--C;
path CD = C--D;
path BE = B--E;

draw(AB);
draw(BC);
draw(CD);
draw(BE, dotted);
label("$\psi_0 = e^{ik_0x} \to$", point((-0.5,-0.5)), NoAlign);
//label("$B$",point((-2,0)),NoAlign);
